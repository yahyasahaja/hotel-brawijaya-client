{
  "reactToolbox": {
    "include": [
      "BUTTON",
      "DATE_PICKER"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-accent": "#ea0f00",
      "color-accent-dark": "#e74c3c",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)",
      "input-text-highlight-color": "#ea0f00",
      "progress-main-color": "#ea0f00",
      "color-primary": "#e74c3c"
    },
    "javascript": "src/assets/theme",
    "styles": "public/static/css"
  },
  "name": "hotel-brawijaya-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.9",
    "apollo-client": "^2.2.5",
    "apollo-link-context": "^1.0.7",
    "apollo-link-http": "^1.5.2",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fast-async": "^6.3.1",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "material-ui": "^0.20.1",
    "mobx": "^3.6.1",
    "mobx-react": "^4.4.2",
    "moment": "^2.21.0",
    "morgan": "^1.9.0",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-css-themr": "^2.1.2",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-image-gallery": "^0.8.7",
    "react-lazyload": "^2.3.0",
    "react-router-dom": "^4.2.2",
    "react-style-proptype": "^3.2.1",
    "react-toolbox": "^2.0.0-beta.12",
    "react-transition-group": "^2.2.0",
    "regenerator": "^0.12.3",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "NODE_ENV=production node build-server/index.js",
    "dev": "node scripts/start.js",
    "build": "node scripts/build.js",
    "dev-server": "NODE_ENV=development nodemon server/index.js --exec babel-node --watch server",
    "build-server": "babel server -d build-server",
    "test": "node scripts/test.js --env=jsdom",
    "postinstall": "yarn build",
    "toolbox": "react-toolbox-themr"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "env"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "react-toolbox-themr": "^1.0.2",
    "sass-loader": "^6.0.6"
  }
}
